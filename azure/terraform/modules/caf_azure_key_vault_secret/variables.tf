// Required Variables
//**********************************************************************************************
variable "caf_key_vault_secret_map" {
  description = "Map of the Key Vault Secrets"
  type = map(object({
    secret_name  = string
    secret_value = string
  }))
  default = {}
}

variable "key_vault_id" {
  description = "Id of the Key vault"
  type        = string
}
